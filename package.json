    "clean": "rm -rf umd es"
  },
  "dependencies": {
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@types/meow": "^5.0.0",
    "@types/node": "^12.12.27",
    "rollup": "^1.31.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "canvas": "^2.6.0"
  }
}
