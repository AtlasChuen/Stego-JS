{
  "name": "stego-js",
  "version": "0.11.0",
  "description": "Steganography, based on frequency domain implemented in JavaScript.",
  "repository": "https://github.com/DimensionDev/Stego-JS",
  "license": "MIT",
  "author": "DimensionDev",
  "main": "es/node.js",
  "browser": "es/dom.js",
  "module": "es/node.js",
  "types": "es/node.d.ts",
  "bin": {
    "stego": "./bin/stego.js"
  },
  "files": [
    "bin",
    "es",
    "umd"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:umd && npm run build:es",
